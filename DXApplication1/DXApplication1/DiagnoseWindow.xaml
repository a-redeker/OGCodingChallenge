<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DXApplication1"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol" x:Class="DXApplication1.DiagnoseWindow"
        mc:Ignorable="d"
        Title="DiagnoseWindow" Height="350" Width="525">
    <Grid>

        <dxlc:FlowLayoutControl HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
            <TextBlock x:Name="textBlock" HorizontalAlignment="Stretch" Margin="0,0,0,0" VerticalAlignment="Stretch" Initialized="textBlock_Initialized"/>
        </dxlc:FlowLayoutControl>

    </Grid>
</Window>
